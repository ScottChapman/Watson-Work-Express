		"request": "^2.85.0",
		"request-debug": "^0.2.0",
		"request-promise": "^4.2.2",
		"timer-machine": "^1.1.0",
		"winston": "^2.4.2"
	},
	"devDependencies": {
		"chai": "^4.1.2",
		"chai-as-promised": "^7.1.1",
		"coveralls": "^3.0.0",
		"istanbul": "^0.4.5",
		"mocha": "^5.1.1",
